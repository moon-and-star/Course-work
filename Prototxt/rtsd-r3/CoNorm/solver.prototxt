
train_net: "./Prototxt/rtsd-r3/CoNorm/train.prototxt"
test_net: "./Prototxt/rtsd-r3/CoNorm/test.prototxt"

test_iter: 30
test_interval: 200

base_lr: 0.0005
lr_policy: "step"
gamma: 0.1
stepsize: 1000
iter_size: 1

momentum: 0.9
weight_decay: 0.0005
display: 1
max_iter: 4000
snapshot: 500
snapshot_prefix: "./snapshots/rtsd-r3/CoNorm/RTSD"
solver_mode: GPU
